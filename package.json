{
  "name": "@myst-tools/theme",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "postinstall": "patch-package",
    "build:thebe": "node copy-thebe-assets.cjs ./public",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "build": "npm run build:css && npm run build:thebe && remix build",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev": "npm run build:thebe && concurrently \"npm run dev:css\" \"remix dev\"",
    "start": "npm run build:thebe && npm run build:css && remix dev"
  },
  "dependencies": {
    "@docsearch/react": "^3.5.0",
    "@headlessui/react": "^1.7.15",
    "@heroicons/react": "^2.0.18",
    "@myst-theme/providers": "^0.3.6",
    "@myst-theme/site": "^0.3.6",
    "@myst-theme/styles": "^0.3.6",
    "@remix-run/node": "^1.17.0",
    "@remix-run/react": "^1.17.0",
    "@remix-run/vercel": "^1.17.0",
    "classnames": "^2.3.2",
    "myst-common": "^1.0.1",
    "myst-config": "^1.0.0",
    "myst-demo": "0.3.6",
    "node-fetch": "^3.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.17.0",
    "@remix-run/eslint-config": "^1.17.0",
    "@remix-run/serve": "^1.17.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node-fetch": "^2.6.4",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "concurrently": "^8.2.0",
    "eslint": "^8.42.0",
    "patch-package": "^7.0.0",
    "prettier": "latest",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.6",
    "vercel": "~28.18.5"
  },
  "packageManager": "npm@8.10.0",
  "engines": {
    "node": ">=16"
  },
  "overrides": {
    "ansi-to-react": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    }
  }
}
