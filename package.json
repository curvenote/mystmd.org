{
  "name": "@myst-tools/theme",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "postinstall": "patch-package",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "build": "npm run build:css && remix build",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "start": "npm run build:css && remix dev"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "@heroicons/react": "^2.0.18",
    "@myst-theme/providers": "^0.2.10",
    "@myst-theme/site": "^0.2.10",
    "@myst-theme/styles": "^0.2.10",
    "@remix-run/node": "^1.16.0",
    "@remix-run/react": "^1.16.0",
    "@remix-run/vercel": "^1.16.0",
    "classnames": "^2.3.2",
    "myst-common": "^0.0.17",
    "myst-config": "^0.0.16",
    "myst-demo": "0.2.10",
    "node-fetch": "^3.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.16.0",
    "@remix-run/eslint-config": "^1.16.0",
    "@remix-run/serve": "^1.16.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node-fetch": "^2.6.3",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "concurrently": "^8.0.1",
    "eslint": "^8.40.0",
    "patch-package": "^7.0.0",
    "prettier": "latest",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3",
    "vercel": "~28.18.5"
  },
  "packageManager": "npm@8.10.0",
  "engines": {
    "node": ">=16"
  },
  "overrides": {
    "ansi-to-react": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    }
  }
}
